
{% extends 'front/frontLayout.html.twig' %}
{% block title %}
    {{ 'front.title.index'|trans({}, 'controller') }}
{% endblock %}
{% block frontContent %}
    <div style='background-image: url("{{ asset('/img/front.jpg') }}")'>
        <img src="{{ asset('/img/front.jpg') }}">
    </div>
    <div class="jumbotron">
        <div id="main" class="container text-center mt-4">
            <h1 class="display-3">{{ 'front.index.text_1'|trans({}, 'controller') }}</h1>
            <div class="lead">{{ 'front.index.text_2'|trans({}, 'controller') }}</div>
            <hr class="my-2">
            <a href="#" class="btn btn-primary"><span class="fas fa-user"></span>{{ 'front.index.login'|trans({}, 'controller') }}</a>
            <a href="#" class="btn btn-success"><span class="fas fa-user-plus"></span>{{ 'front.index.signup'|trans({}, 'controller') }}</a>
        </div>
    </div>
{% endblock %}